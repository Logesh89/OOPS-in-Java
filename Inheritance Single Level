class parent{
    int a=10;
    void display(){
        System.out.println("Parent class");
    }
}
class child extends parent{
    int b=20;
    void show(){
        System.out.println("Child class");
    }
}

class Main{
    public static void main(String[] args){
        child c=new child();
        System.out.println(c.b);
        c.show();
        System.out.println(c.a);
        c.display();
    }
}
